<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="com.cortexits.pharmacy.sample.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
   
       <Label fx:id="welcomeText" />
       <Button onAction="#onHelloButtonClick" text="Hello!" />
      <ToggleButton mnemonicParsing="false" selected="true" text="ToggleButton">
         <toggleGroup>
            <ToggleGroup fx:id="tg" />
         </toggleGroup>
      </ToggleButton>
      <ToggleButton mnemonicParsing="false" text="ToggleButton" toggleGroup="$tg" />
   </children>
</VBox>
